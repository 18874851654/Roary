#!/usr/bin/env perl
use Moose;
use Getopt::Long;
my ($output_filename) ;
GetOptions(
    'query=s'    => \$output_filename,
    'db=s'    => \$output_filename,
    'evalue=s'    => \$output_filename,
    'num_threads=s'    => \$output_filename,
    'outfmt=s'    => \$output_filename,
    'max_target_seqs=s'    => \$output_filename,
    
);
system("touch _blast_results");
1;